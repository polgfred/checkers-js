{
  "name": "checkers",
  "version": "2.0.0",
  "description": "Checkers in JavaScript",
  "scripts": {
    "clean": "rm -rf lib static/*-bundle.{js,js.map}",
    "build:client": "NODE_ENV=production webpack",
    "build:server": "NODE_ENV=production tsc",
    "start": "NODE_ENV=production node --require esm ./lib/server.js",
    "start:dev": "NODE_ENV=development node --require ts-node/register ./src/server.ts",
    "test": "jest",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "lint": "eslint --config .eslintrc.js ./src",
    "types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/polgfred/checkers-js.git"
  },
  "author": "Frederick Polgardy <fred@polgardy.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "classnames": "^2.5.1",
    "core-js": "^3.40.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-postcss-modules": "^2.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "regenerator-runtime": "^0.14.1",
    "style-loader": "^4.0.0",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.2"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "express": "^4.21.2"
  }
}
