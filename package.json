    "@typescript-eslint/parser": "^4.8.2",
    "classnames": "^2.2.6",
    "core-js": "^3.7.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
