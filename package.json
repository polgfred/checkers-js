    "start:dev": "WEBPACK_DEV_SERVER=true ts-node ./src/server.ts",
    "test": "jest",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "lint": "eslint --config .eslintrc.js ./src"
  },
  "repository": {
